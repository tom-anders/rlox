class Scone {
    topping(first, second) {
        print "scone with " + first + " and " + second;
    }
}

var scone = Scone();
scone.topping("berries", "cream"); // expect: scone with berries and cream
var topping = scone.topping;
topping("berries", "cream"); // expect: scone with berries and cream

